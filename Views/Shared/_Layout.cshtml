    <script src="http://localhost:2668/Scripts/AngularJsControllers/DataGridService.js"></script>
    <script src="http://localhost:2668/Scripts/AngularJsControllers/angular-block-ui.js"></script>


    <script src="~/Scripts/AngularJsControllers/ajaxService1.js"></script>
    <script src="~/Scripts/AngularJsControllers/ajaxService2.js"></script>
    <script src="~/Scripts/AngularJsControllers/CustomerEntryDetails.js"></script>
   

</body>
